node {
     ws("${JENKINS_HOME}/jobs/${JOB_NAME}/builds/${BUILD_ID}/src/LabOne") {
        withEnv(["GOROOT=/home/palani/go", "GOPATH=${JENKINS_HOME}/jobs/${JOB_NAME}/builds/${BUILD_ID}/", "PATH+GO=/usr/local/go/bin"]) {
            env.PATH="${GOPATH}/bin:$PATH"

            stage 'Checkout'

            stage 'preTest'
            sh 'go version'
            sh 'dep init'

            stage 'Test'
            sh 'go vet'
            sh 'go test -cover'

            stage 'Build'
            sh 'go build .'

            stage 'Deploy'
            // Do nothing.
        }
    }
}
